<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - Malermester Laurids Vestergaard</title>
    <link href="~/Content/CSS/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/CSS/Site.css" rel="stylesheet" />

    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-72670413-1', 'auto');
        ga('send', 'pageview');

    </script>

    @if (IsSectionDefined("AddToHead"))
    {
        @RenderSection("AddToHead", required: false)
    }
</head>
<body>
    <div class="navbar" id="menuWrap">
        <div class="container">
            <div id="companyLogo">
                <h3 class="nomargin nopadding">
                    Malermester Laurids Vestergaard
                </h3>
            </div>
        </div>
        <div class="container" id="menu">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="/Home/Forside" id="Forside">Forside</a>
                    </li>
                    <li>
                        <a href="/Home/OmMig" id="OmMig">Om Mig</a>
                    </li>
                    <li>
                        <a href="/Home/FlytteLejligheder" id="FlytteLejligheder">Flytte Lejligheder</a>
                    </li>
                    <li>
                        <a href="/Home/Private" id="Private">Private</a>
                    </li>
                    <li>
                        <a href="/Home/Erhverv" id="Erhverv">Erhverv</a>
                    </li>
                    <li>
                        <a href="/Home/Galleri" id="Galleri">Galleri</a>
                    </li>
                    <li>
                        <a href="/Home/Kontakt" id="Kontakt">Kontakt</a>
                    </li>

                </ul>
            </div>
        </div>
    </div>
    <div class="container nopadding">
        <div id="logoWrapper">
            <div id="logo">
                <p>@ViewBag.Title</p>
            </div>
            <div id="social">
                <ul>
                    <li>
                        <a href="https://www.facebook.com/Malermester-Laurids-Vestergaard-353273034865774/?fref=ts" target="_blank" rel="nofollow"><img src="~/Content/Images/Layout/facebook.png" alt="" /></a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row" id="bodyWrapper">
            <div class="col-xs-12" id="pageTitleMobile">
                <p class="page-header">
                    @ViewBag.Title
                </p>
            </div>
            <div class="col-md-12">
                <div class="row">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <div class="container-fluid" id="footerWrapper">
        <footer class="container">
            <div class="row">
                <div class="col-sm-4">
                    <div class="footerTitle">
                        <h2>Maler Produkter</h2>
                    </div>
                    <div class="footerContent">
                        <div class="row">
                            <a href="http://www.bj.dk/" rel="nofollow" target="_blank">
                                <div class="col-xs-3">
                                    <img src="~/Content/Images/Layout/bj_logo.png" />
                                </div>
                                <div class="col-xs-9">
                                    Beck & Jørgensen A/S
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="footerTitle">
                        <h2>Garantier</h2>
                    </div>
                    <div class="footerContent">
                        <div class="row">
                            <a href="http://www.malermestre.dk/" rel="nofollow" target="_blank">
                                <div class="col-xs-3">
                                    <img src="~/Content/Images/Layout/Logo_DM.jpg" />
                                </div>
                                <div class="col-xs-9">
                                    Danske Malermestre
                                </div>
                            </a>
                        </div>
                        <div class="row">
                            <a href="http://www.aarhusmalerlaug.dk/" rel="nofollow" target="_blank">
                                <div class="col-xs-3">
                                    <img src="~/Content/Images/Layout/malerlaug.jpg" />
                                </div>
                                <div class="col-xs-9">
                                    Aarhus Malerlaug
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="footerTitle">
                        <h2>Lavet af</h2>
                    </div>
                    <div class="footerContent">
                        <p>
                            &copy; @DateTime.Now.Year - Proper Dog ApS
                        </p>
                        <div class="footerButton">
                            <a href="http://www.Proper-Dog.com" rel="nofollow" target="_blank">Mere Info</a>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.2/jquery.min.js"></script>
    <script src="~/Content/Scripts/bootstrap.js"></script>
    <script src="~/Content/Scripts/SelectedActive.js"></script>
    @RenderSection("AddToButtomHTML", false)
</body>
</html>
